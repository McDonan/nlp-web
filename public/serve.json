{
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "deny"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Cache-Control",
          "value": "max-age=0, must-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
